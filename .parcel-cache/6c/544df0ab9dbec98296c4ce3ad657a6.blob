\r\n}\r\n\r\n/*lists*/\r\n\r\n/*-alignment and sizing*/\r\n.center-items {\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: center;\r\n}\r\n\r\n.center-row {\r\n    display: flex;\r\n    align-items: center;\r\n    flex-direction: column;\r\n}\r\n\r\n.h-space {\r\n    margin: 0 50px;\r\n}\r\n\r\n.v-space {\r\n    margin-top: 10%;\r\n}\r\n\r\n.em-indent {\r\n    margin-left: 1em;\r\n}\r\n\r\n.en-indent {\r\n    text-indent: 0.5em;\r\n}\r\n\r\nsection {\r\n    padding: 5vh 15% 0;\r\n    box-sizing: border-box;\r\n    display: block;\r\n    overflow: hidden;\r\n}\r\n\r\n.space-around {\r\n    display: flex;\r\n    justify-content: space-around;\r\n    flex-wrap: wrap;\r\n}\r\n\r\n#index .single-column {\r\n    min-width: 20em;\r\n    max-width: 43%;\r\n}\r\n\r\n.single-column {\r\n    max-width: 30em;\r\n}\r\n\r\n.double-column {\r\n    column-count: 2;\r\n    text-align: left;\r\n}\r\n\r\n/*animations*/\r\n\r\n.fade-slide-right {\r\n    animation: fade-in 1.5s forwards, slide-right 1.5s;\r\n}\r\n\r\n.fade-in {\r\n    animation: fade-in 3s forwards;\r\n}\r\n\r\n@keyframes fade-in {\r\n    from {opacity: 0;}\r\n    to {opacity: 1;}\r\n}\r\n\r\n@keyframes lock {\r\n    from {\r\n        height: 100vh;\r\n        overflow: hidden;\r\n    }\r\n    to {\r\n        height: fit-content;\r\n        overflow-y: scroll;\r\n    }\r\n}\r\n\r\n@keyframes slide-right {\r\n    from {transform: translateX(-5em);}\r\n    to {transform: translateX(0);}\r\n}\r\n\r\n@keyframes bounce-down {\r\n    from {transform: translateY(-120%);}  \r\n    60% {transform: translateY(15%);}\r\n    to {transform: translateY(0);}\r\n}\r\n\r\n@keyframes unveil-down {\r\n    from {\r\n        height: 100%;\r\n    }\r\n    to {\r\n        height: 0;\r\n        visibility: hidden;\r\n    }\r\n}\r\n\r\n@keyframes bounce-up {\r\n    from {transform: translateY(100vh);}\r\n    60% {transform: translateY(-15%);}\r\n    to {transform: translateY(0);}\r\n}\r\n\r\n\r\n@keyframes float1 {\r\n